    "queries": {
      "cassandra.reads": {
        "name": "cassandra.reads",
        "targets": [
          "summarize(cassandra.total_operations-reads, \"12s\", \"avg\")"
        ]
      },
      "cassandra.total": {
        "name": "cassandra.total",
        "targets": [
          "summarize(cassandra.total_operations-all, \"12s\", \"avg\")"
        ]
      },
      "cassandra.total.pernode": {
        "name": "cassandra.total.pernode",
        "targets": [
          "aliasByNode(summarize(*.cassandra-0.total_operations-all, \"12s\", \"avg\"), 0)"
        ]
      },
      "cassandra.writes": {
        "name": "cassandra.writes",
        "targets": [
          "summarize(cassandra.total_operations-writes, \"12s\", \"avg\")"
        ]
      },
      "disk.reads": {
        "name": "disk.reads",
        "targets": [
          "aliasByNode(summarize(*.disk-sda.disk_octets.read, \"10s\", \"avg\"), 0)"
        ]
      },
      "disk.writes": {
        "name": "disk.writes",
        "targets": [
          "aliasByNode(summarize(*.disk-sda.disk_octets.write, \"10s\", \"avg\"), 0)"
        ]
      },
      "memory.cached": {
        "name": "memory.cached",
        "targets": [
          "aliasByNode(summarize(*.memory.memory-cached, \"10s\", \"avg\"), 0)"
